<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg03"
    tools:context=".north_east">

    <TextView
        android:id="@+id/txt_east"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="NORTH EAST"
        android:textColor="#03D2BE"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_9sl" />

    <TextView
        android:id="@+id/txt_9sl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Sri Lanka"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="332dp"
        android:layout_height="496dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_east"
        app:layout_constraintVertical_bias="0.829"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fillViewport="true"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imgbtn_ampara"
                android:layout_width="330dp"
                android:layout_height="108dp"
                android:contentDescription="View"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.51"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imgbtn_trinco"
                app:layout_constraintVertical_bias="0.218"
                app:srcCompat="@drawable/ampara" />

            <TextView
                android:id="@+id/txt_ampara"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="AMPARA"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold|italic" />

            <ImageButton
                android:id="@+id/imgbtn_batticaloa"
                android:layout_width="330dp"
                android:layout_height="108dp"
                android:contentDescription="View"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.51"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imgbtn_ampara"
                app:layout_constraintVertical_bias="0.395"
                app:srcCompat="@drawable/batticaloa" />

            <TextView
                android:id="@+id/txt_batticaloa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="BATTICOLOA"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold|italic" />

            <ImageButton
                android:id="@+id/imgbtn_trinco"
                android:layout_width="330dp"
                android:layout_height="108dp"
                android:contentDescription="View"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.51"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_east2"
                app:layout_constraintVertical_bias="0.109"
                app:srcCompat="@drawable/trinco" />

            <TextView
                android:id="@+id/txt_trinco"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TRINCOMALE"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold|italic" />

            <ImageButton
                android:id="@+id/imgbtn_jaffna"
                android:layout_width="330dp"
                android:layout_height="108dp"
                android:contentDescription="View"
                app:layout_constraintBottom_toTopOf="@+id/imgbtn_mullaitivu"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.561"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_north2"
                app:layout_constraintVertical_bias="0.12"
                app:srcCompat="@drawable/jaffna" />

            <TextView
                android:id="@+id/txt_jaffna"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="JAFFNA"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold|italic" />

            <ImageButton
                android:id="@+id/imgbtn_mannar"
                android:layout_width="330dp"
                android:layout_height="108dp"
                android:contentDescription="View"
                app:layout_constraintBottom_toTopOf="@+id/txt_vavuniya"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.573"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_north2"
                app:layout_constraintVertical_bias="0.372"
                app:srcCompat="@drawable/mannar" />

            <TextView
                android:id="@+id/txt_mannar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="MANNAR"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold|italic" />

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>